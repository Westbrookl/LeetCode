
public class Suolution {
	public int myAoti(String s){
		int result=0;
		int length = s.length();
		if(s==""&& s.length()==0) return 0;
		
		for(int i=0;i<length;i++){
			if(s.charAt(i) == ' '){
				continue;
			}else{
				s = s.substring(i, length);
				break;
			}
		}
		if(s.charAt(0) == '+' || s.charAt(0) == '-' || (s.charAt(0)>='0'&& s.charAt(0)<='9')){
			for(int i=1;i<)
		}else{
			result = 0;
		}1
		
		return result;
	}
}
